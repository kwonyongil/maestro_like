<script>


//1
var writers = new Array("Maestro","Maestro","Maestro","Maestro")
<% for j in 0..3 do %>
    <% if @writer[j] != nil %>
        writers[<%=j%>] = "<%= @writer[j] %>"
    <%end%>
<%end%>


var m = 0;
$(function () {
    var $aud = new Array();
    for (i=0;i<4;i++) {
      $aud.push($(".audio__playlist_"+i));
    }
    
    $.each($aud,function(k){
        $aud[k].each(function(i){
            var $vol = $("#volume_" + k + "_" + i);
            var AUDIO = $aud[k][i];
            var volvalue = <%= @volume %>;
            
            AUDIO.volume = volvalue[k][i];
        
            $vol.slider({
                min:0,
                max: 100,
                range :"min",
                value: AUDIO.volume * 100,
                slide: function (ev, ui) {
                
                    AUDIO.volume = ui.value / 100;
                }
            })
        });
    });
});

$(function () {
    var $default_aud = new Array();
    for (i=0;i<4;i++) {
      $default_aud.push($(".audio__playlist__"+i));
    }
    
    $.each($default_aud,function(k){
        $default_aud[k].each(function(i){
            var $vol = $("#volume_" + k + "__" + i);
            var AUDIO = $default_aud[k][i];
            var volvalue_2 =  <%= @default_volume_array %>;
            AUDIO.volume = volvalue_2[k][i];
            
        
            $vol.slider({
                value: AUDIO.volume * 100,
                min:0,
                max: 100,
                range :"min",
                
                slide: function (ev, ui) {
           
                    AUDIO.volume = ui.value / 100;
                }
            })
        });
        
    });
});
function soundoff(sound) {
var thesound=document.getElementsByClassName(sound);
    $.each(thesound,function(i){
        thesound[i].pause();
    });
}

function PlaySound(soundobj) {
var thissound=document.getElementsByClassName(soundobj);
if (m == 0){
    $.each(thissound,function(i){
    thissound[i].pause();
    });
    $(".playlist-play-icon").attr('src', '/audio_off_icon.png');
    m=1;
}
else
{
    $.each(thissound,function(i){
    thissound[i].play();
    });
    $(".playlist-play-icon").attr('src', '/audio_on_icon.png');
      m=0;
    }
}

function modalGo(){
    $('#Modal').attr('data-target','#myModal');
    $('#soundimg').attr('onClick',"PlaySound('audio__playlist_0')");
   
    // db variable
