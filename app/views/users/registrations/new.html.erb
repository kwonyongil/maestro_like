
<div class="main_source">
<div class="container">
  <div class="row">
      <div class="logo-box">
        <h3 class="logo">Maestro</h3>
      </div>
  <div class="container">
    <div class="row">
      <div class="col-sm-4"></div>
      <div class="login-box col-sm-4" id="login_ajax">
        <h2 class="small-title">sign up Maestro</h2>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
      
          <div class="field">
            <%= f.text_field :name, autofocus: true, placeholder:"user name", class:"input-box"%>
          </div>
        
          <div class="field">
            <%= f.email_field :email, autofocus: true, placeholder:"user email", class:"input-box" %>
          </div>
        
          <div class="field">
            <%= f.password_field :password, autocomplete: "off", placeholder:"password", class:"input-box" %>
          </div>
        
          <div class="field">
            <%= f.password_field :password_confirmation, autocomplete: "off", placeholder:"password confirmation", class:"input-box"  %>
          </div>
          <div class="field">
            <%= f.submit "sign up", class:"sign-up-page-button" %>
        <% end %>
          </div>
        
      </div>  <!--아약스 -->
      <div class="col-sm-4"></div>
    </div>
  </div>
</div>
</div>
</div>
<video autoplay loop id="video-background" muted plays-inline>
  <source src="https://s3.ap-northeast-2.amazonaws.com/kwonyongil/uploads/maestro_login/short.mp4" type="video/mp4">
  <source src="https://s3.ap-northeast-2.amazonaws.com/kwonyongil/uploads/maestro_login/short.webm" type="video/webm">
	<source src="https://s3.ap-northeast-2.amazonaws.com/kwonyongil/uploads/maestro_login/short.ogg" type="video/ogg">
</video>
<style>
body {
  
    background-repeat: no-repeat;
    background-size: cover;
}
</style>

<audio src="/Forwards_Tom+Day.mp3" autoplay="autoplay"  display="none"></audio>