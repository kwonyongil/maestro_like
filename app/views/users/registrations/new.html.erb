
<div class="main_source">
<div class="container">
  <div class="row">
      <div class="logo-box">
        <h3 class="logo">Maestro</h3>
      </div>
  <div class="container">
    <div class="row">
      <div class="col-sm-4"></div>
      <div class="login-box col-sm-4">
  <h2 class="small-title">sign up Maestro</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.text_field :name, autofocus: true, placeholder:"user name", class:"input-box"%>
  </div>

  <div class="field">
    <%= f.email_field :email, autofocus: true, placeholder:"user email", class:"input-box" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "off", placeholder:"password", class:"input-box" %>
  </div>

  <div class="field">
    <%= f.password_field :password_confirmation, autocomplete: "off", placeholder:"password confirmation", class:"input-box"  %>
  </div>
  <div class="field">
    <%= f.submit "sign up", class:"sign-up-page-button" %>
<% end %>
</div>

</div>
      <div class="col-sm-4"></div>
    </div>
  </div>
</div>
</div>
</div>
<video autoplay loop id="video-background" muted plays-inline>
  <source src="/short.mp4" type="video/mp4">
  <source src="/short.webm" type="video/webm">
	<source src="/short.ogg" type="video/ogg">
</video>
<style>
body {
  
    background-repeat: no-repeat;
    background-size: cover;
}
</style>
